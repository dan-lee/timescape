    "@types/react-dom": "18.3.0",
    "@vanilla-extract/css": "1.16.0",
    "@vanilla-extract/vite-plugin": "4.0.16",
    "@vitejs/plugin-react": "4.3.2",
    "@vitejs/plugin-vue": "5.1.4",
    "concurrently": "9.0.1",
    "storybook": "8.3.5",
    "storybook-addon-code-editor": "3.1.0",
    "vite": "5.4.8",
