{
  "private": true,
  "name": "demo",
  "version": "1.3.5",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "concurrently -n 'integration,storybook' 'pnpm vite --port 4949' 'pnpm storybook:dev --no-open'",
    "dev:integrations": "vite build && concurrently 'pnpm storybook:build' 'vite build -l error -w'",
    "build": "pnpm typecheck && vite build && pnpm storybook:build",
    "preview": "vite preview",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build -o storybook-dist",
    "storybook:test": "storybook test",
    "test": "playwright test"
  },
  "dependencies": {
    "@rehookify/datepicker": "6.6.8",
    "htm": "3.1.1",
    "preact": "10.27.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "solid-js": "1.9.9",
    "svelte": "4.2.19",
    "timescape": "workspace:*",
    "vue": "3.5.19"
  },
  "devDependencies": {
    "@iconify-icon/react": "3.0.0",
    "@playwright/test": "1.55.0",
    "@storybook/react-vite": "9.1.2",
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@types/node": "22.7.5",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@vanilla-extract/css": "1.17.4",
    "@vanilla-extract/vite-plugin": "4.0.16",
    "@vitejs/plugin-react": "5.0.1",
    "@vitejs/plugin-vue": "6.0.1",
    "concurrently": "9.2.0",
    "storybook": "9.1.2",
    "storybook-addon-code-editor": "^5.0.0",
    "vite": "7.1.3",
    "vite-plugin-top-level-await": "1.6.0"
  }
}
