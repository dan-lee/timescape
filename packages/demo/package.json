{
  "private": true,
  "name": "demo",
  "version": "1.3.4",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "concurrently -n 'integration,storybook' 'pnpm vite --port 4949' 'pnpm storybook:dev --no-open'",
    "dev:integrations": "vite build && concurrently 'pnpm storybook:build' 'vite build -l error -w'",
    "build": "pnpm typecheck && vite build && pnpm storybook:build",
    "preview": "vite preview",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build -o storybook-dist",
    "storybook:test": "storybook test"
  },
  "dependencies": {
    "@preact/signals": "1.3.0",
    "@rehookify/datepicker": "6.6.7",
    "htm": "3.1.1",
    "preact": "10.24.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "solid-js": "1.9.2",
    "svelte": "4.2.19",
    "timescape": "workspace:*",
    "vue": "3.5.11"
  },
  "devDependencies": {
    "@iconify-icon/react": "2.1.0",
    "@storybook/addon-a11y": "8.3.5",
    "@storybook/addon-controls": "8.3.5",
    "@storybook/addon-essentials": "8.3.5",
    "@storybook/addon-interactions": "8.3.5",
    "@storybook/addon-links": "8.3.5",
    "@storybook/blocks": "8.3.5",
    "@storybook/manager-api": "8.3.5",
    "@storybook/react": "8.3.5",
    "@storybook/react-vite": "8.3.5",
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@types/node": "22.7.5",
