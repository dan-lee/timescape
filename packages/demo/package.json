{
  "private": true,
  "name": "demo",
  "version": "1.2.7",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "concurrently -n 'integration,storybook' 'pnpm vite --port 4949' 'pnpm storybook:dev --no-open'",
    "dev:integrations": "vite build && concurrently 'pnpm storybook:build' 'vite build -l error -w'",
    "build": "pnpm typecheck && vite build && pnpm storybook:build",
    "preview": "vite preview",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build -o storybook-dist",
    "storybook:test": "storybook test"
  },
  "dependencies": {
    "@preact/signals": "1.2.3",
    "@rehookify/datepicker": "6.6.1",
    "htm": "^3.1.1",
    "preact": "^10.22.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "solid-js": "^1.8.17",
    "svelte": "^4.2.18",
    "timescape": "workspace:*",
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@iconify-icon/react": "2.1.0",
    "@storybook/addon-a11y": "8.2.9",
    "@storybook/addon-controls": "8.2.9",
    "@storybook/addon-essentials": "8.2.9",
    "@storybook/addon-interactions": "8.2.9",
    "@storybook/addon-links": "8.2.9",
    "@storybook/blocks": "8.2.9",
    "@storybook/manager-api": "8.2.9",
    "@storybook/react": "8.2.9",
    "@storybook/react-vite": "8.2.9",
    "@sveltejs/vite-plugin-svelte": "3.1.1",
    "@types/node": "20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vanilla-extract/css": "^1.15.2",
    "@vanilla-extract/vite-plugin": "^4.0.10",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "concurrently": "^8.2.2",
    "storybook": "8.2.9",
    "storybook-addon-code-editor": "^3.0.0",
    "vite": "5.4.1",
    "vite-plugin-top-level-await": "1.4.4"
  }
}
